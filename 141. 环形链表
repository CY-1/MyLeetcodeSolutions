# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, x):
#         self.val = x
#         self.next = None

class Solution:
    def hasCycle(self, head: ListNode) -> bool:
        def my(tree):
            if tree==None:
                return False
            else:
                if tree.val==None:
                    return True
                else:
                    tree.val=None
                    return my(tree.next)
        return my(head)
# 遍历的过程中修改val值表示来过 即可
